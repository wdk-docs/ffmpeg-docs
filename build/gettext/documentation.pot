msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:3461
msgid "dpm integer Set physical density of pixels, in dots per meter, unset by default"
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:3464
msgid "9.15 ProRes Apple ProRes encoder."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:3467
msgid "FFmpeg contains 2 ProRes encoders, the prores-aw and prores-ks encoder. The used encoder can be chosen with the -vcodec option."
msgstr ""

