#: ../../source/documentation/components/ffmpeg-filters.rst:13553
msgid "} x /= 3; y /= 3;"
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:13556
msgid "}"
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:13558
