#: ../../source/documentation/components/ffmpeg-filters.rst:7809
msgid "‘cosine’ Interpolate values using the cosine interpolation."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:7812
msgid "‘cubic’ Interpolate values using the cubic interpolation."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:7815
msgid "‘spline’ Interpolate values using the spline interpolation."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:7818
msgid "10.111 lut3d Apply a 3D LUT to an input video."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:7823
msgid "file Set the 3D LUT file name."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:7828
msgid "‘3dl’ AfterEffects"
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:7834
msgid "‘dat’ DaVinci"
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:7837
msgid "‘m3d’ Pandora"
