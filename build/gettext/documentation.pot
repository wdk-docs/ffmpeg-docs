#: ../../source/documentation/components/ffmpeg-filters.rst:8657
msgid "t Set scope tilt/rotation."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:8660
msgid "o Set trace opacity."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:8663
msgid "tx Set trace center x position."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:8666
msgid "ty Set trace center y position."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:8669
msgid "tw Set trace width, relative to width of frame."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:8672
msgid "th Set trace height, relative to height of frame."
