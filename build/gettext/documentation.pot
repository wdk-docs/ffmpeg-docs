msgid "ffmpeg -i INPUT -c:v libvpx -ts-parameters ts_number_layers=3:\\ ts_target_bitrate=250000,500000,1000000:ts_rate_decimator=4,2,1:\\ ts_periodicity=4:ts_layer_id=0,2,1,2 OUTPUT Below is a brief explanation of each of the parameters, please refer to struct vpx_codec_enc_cfg in vpx/vpx_encoder.h for more details."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:2852
msgid "ts_number_layers Number of temporal coding layers."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:2855
msgid "ts_target_bitrate Target bitrate for each temporal layer."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:2858
