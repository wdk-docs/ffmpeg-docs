msgstr ""

#: ../../source/documentation/components/ffmpeg-formats.rst:1706
msgid "‘advanced_codec_digital_radio’ Advanced Codec Digital Radio service."
msgstr ""

#: ../../source/documentation/components/ffmpeg-formats.rst:1709
msgid "‘mpeg2_digital_hdtv’ MPEG2 Digital HDTV service."
msgstr ""

#: ../../source/documentation/components/ffmpeg-formats.rst:1712
msgid "‘advanced_codec_digital_sdtv’ Advanced Codec Digital SDTV service."
msgstr ""

#: ../../source/documentation/components/ffmpeg-formats.rst:1715
msgid "‘advanced_codec_digital_hdtv’ Advanced Codec Digital HDTV service."
msgstr ""

#: ../../source/documentation/components/ffmpeg-formats.rst:1718
msgid "mpegts_pmt_start_pid integer Set the first PID for PMT. Default is 0x1000. Max is 0x1f00."
