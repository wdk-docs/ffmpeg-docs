#: ../../source/documentation/components/ffmpeg-protocols.rst:616
msgid "rtcpport=n Set the remote RTCP port to n."
msgstr ""

#: ../../source/documentation/components/ffmpeg-protocols.rst:619
msgid "localrtpport=n Set the local RTP port to n."
msgstr ""

#: ../../source/documentation/components/ffmpeg-protocols.rst:622
msgid "localrtcpport=n' Set the local RTCP port to n."
msgstr ""

#: ../../source/documentation/components/ffmpeg-protocols.rst:625
msgid "pkt_size=n Set max packet size (in bytes) to n."
msgstr ""

#: ../../source/documentation/components/ffmpeg-protocols.rst:628
msgid "connect=0|1 Do a connect() on the UDP socket (if set to 1) or not (if set to 0)."
msgstr ""

#: ../../source/documentation/components/ffmpeg-protocols.rst:631
msgid "sources=ip[,ip] List allowed source IP addresses."
msgstr ""

#: ../../source/documentation/components/ffmpeg-protocols.rst:634
msgid "block=ip[,ip] List disallowed (blocked) source IP addresses."
msgstr ""

#: ../../source/documentation/components/ffmpeg-protocols.rst:637
msgid "write_to_source=0|1 Send packets to the source address of the latest received packet (if set to 1) or to a default remote address (if set to 0)."
