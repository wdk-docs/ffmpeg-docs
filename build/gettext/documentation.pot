#: ../../source/documentation/components/ffmpeg-filters.rst:13912
msgid "‘replace’ Replace old columns with new ones."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:13915
msgid "‘scroll’ Scroll from right to left."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:13918
msgid "‘rscroll’ Scroll from left to right."
msgstr ""

#: ../../source/documentation/components/ffmpeg-filters.rst:13921
msgid "‘picture’ Draw single picture."
msgstr ""

