#: ../../source/documentation/components/ffmpeg-codecs.rst:2420
msgid "frame_size (--blocksize) Default is 32768."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:2423
msgid "compression_level Set speed vs. compression tradeoff. Acceptable arguments are listed below:"
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:2426
msgid "‘0 (-f)’ Fast mode."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:2429
msgid "‘1’ Normal (default) settings."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:2432
msgid "‘2 (-h)’ High quality."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:2435
msgid "‘3 (-hh)’ Very high quality."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:2438
msgid "‘4-8 (-hh -xEXTRAPROC)’ Same as ‘3’, but with extra processing enabled."
