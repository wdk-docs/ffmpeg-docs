#: ../../source/documentation/components/ffmpeg-codecs.rst:3349
msgid "‘mv4’ Use four motion vector by macroblock."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:3352
msgid "‘aic’ Enable high quality AC prediction."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:3355
msgid "‘gray’ Only encode grayscale."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:3358
msgid "‘gmc’ Enable the use of global motion compensation (GMC)."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:3361
msgid "‘qpel’ Enable quarter-pixel motion compensation."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:3364
msgid "‘cgop’ Enable closed GOP."
msgstr ""

#: ../../source/documentation/components/ffmpeg-codecs.rst:3370
msgid "trellis me_method Set motion estimation method. Possible values in decreasing order of speed and increasing order of quality:"
