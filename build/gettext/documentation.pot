#: ../../source/documentation/components/ffmpeg-formats.rst:1691
msgid "mpegts_service_type integer Set the program ‘service_type’. Default is digital_tv. Accepts the following options:"
msgstr ""

#: ../../source/documentation/components/ffmpeg-formats.rst:1694
msgid "‘hex_value’ Any hexadecimal value between 0x01 and 0xff as defined in ETSI 300 468."
msgstr ""

#: ../../source/documentation/components/ffmpeg-formats.rst:1697
msgid "‘digital_tv’ Digital TV service."
msgstr ""

#: ../../source/documentation/components/ffmpeg-formats.rst:1700
msgid "‘digital_radio’ Digital Radio service."
msgstr ""

#: ../../source/documentation/components/ffmpeg-formats.rst:1703
