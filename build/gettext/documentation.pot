#: ../../source/documentation/components/ffmpeg-resampler.rst:13
msgid "The FFmpeg resampler provides a high-level interface to the libswresample library audio resampling utilities. In particular it allows one to perform audio resampling, audio channel layout rematrixing, and convert audio format and packing layout."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:16
msgid "2 Resampler Options"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:18
msgid "The audio resampler supports the following named options."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:20
msgid "Options may be set by specifying -option value in the FFmpeg tools, option=value for the aresample filter, by setting the value explicitly in the SwrContext options or using the libavutil/opt.h API for programmatic use."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:22
msgid "ich, in_channel_count Set the number of input channels. Default value is 0. Setting this value is not mandatory if the corresponding channel layout in_channel_layout is set."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:25
msgid "och, out_channel_count Set the number of output channels. Default value is 0. Setting this value is not mandatory if the corresponding channel layout out_channel_layout is set."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:28
msgid "uch, used_channel_count Set the number of used input channels. Default value is 0. This option is only used for special remapping."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:31
msgid "isr, in_sample_rate Set the input sample rate. Default value is 0."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:34
msgid "osr, out_sample_rate Set the output sample rate. Default value is 0."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:37
msgid "isf, in_sample_fmt Specify the input sample format. It is set by default to none."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:40
msgid "osf, out_sample_fmt Specify the output sample format. It is set by default to none."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:43
msgid "tsf, internal_sample_fmt Set the internal sample format. Default value is none. This will automatically be chosen when it is not explicitly set."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:46
msgid "icl, in_channel_layout ocl, out_channel_layout Set the input/output channel layout."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:52
msgid "clev, center_mix_level Set the center mix level. It is a value expressed in deciBel, and must be in the interval [-32,32]."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:55
msgid "slev, surround_mix_level Set the surround mix level. It is a value expressed in deciBel, and must be in the interval [-32,32]."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:58
msgid "lfe_mix_level Set LFE mix into non LFE level. It is used when there is a LFE input but no LFE output. It is a value expressed in deciBel, and must be in the interval [-32,32]."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:61
msgid "rmvol, rematrix_volume Set rematrix volume. Default value is 1.0."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:64
msgid "rematrix_maxval Set maximum output value for rematrixing. This can be used to prevent clipping vs. preventing volume reduction. A value of 1.0 prevents clipping."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:67
msgid "flags, swr_flags Set flags used by the converter. Default value is 0."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:70
msgid "It supports the following individual flags:"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:72
msgid "res force resampling, this flag forces resampling to be used even when the input and output sample rates match."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:75
msgid "dither_scale Set the dither scale. Default value is 1."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:78
msgid "dither_method Set dither method. Default value is 0."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:83
msgid "‘rectangular’ select rectangular dither"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:86
msgid "‘triangular’ select triangular dither"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:89
msgid "‘triangular_hp’ select triangular dither with high pass"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:92
msgid "‘lipshitz’ select Lipshitz noise shaping dither."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:95
msgid "‘shibata’ select Shibata noise shaping dither."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:98
msgid "‘low_shibata’ select low Shibata noise shaping dither."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:101
msgid "‘high_shibata’ select high Shibata noise shaping dither."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:104
msgid "‘f_weighted’ select f-weighted noise shaping dither"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:107
msgid "‘modified_e_weighted’ select modified-e-weighted noise shaping dither"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:110
msgid "‘improved_e_weighted’ select improved-e-weighted noise shaping dither"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:113
msgid "resampler Set resampling engine. Default value is swr."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:118
msgid "‘swr’ select the native SW Resampler; filter options precision and cheby are not applicable in this case."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:121
msgid "‘soxr’ select the SoX Resampler (where available); compensation, and filter options filter_size, phase_shift, exact_rational, filter_type & kaiser_beta, are not applicable in this case."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:124
msgid "filter_size For swr only, set resampling filter size, default value is 32."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:127
msgid "phase_shift For swr only, set resampling phase shift, default value is 10, and must be in the interval [0,30]."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:130
msgid "linear_interp Use linear interpolation when enabled (the default). Disable it if you want to preserve speed instead of quality when exact_rational fails."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:133
msgid "exact_rational For swr only, when enabled, try to use exact phase_count based on input and output sample rate. However, if it is larger than 1 << phase_shift, the phase_count will be 1 << phase_shift as fallback. Default is enabled."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:136
msgid "cutoff Set cutoff frequency (swr: 6dB point; soxr: 0dB point) ratio; must be a float value between 0 and 1. Default value is 0.97 with swr, and 0.91 with soxr (which, with a sample-rate of 44100, preserves the entire audio band to 20kHz)."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:139
msgid "precision For soxr only, the precision in bits to which the resampled signal will be calculated. The default value of 20 (which, with suitable dithering, is appropriate for a destination bit-depth of 16) gives SoX’s ’High Quality’; a value of 28 gives SoX’s ’Very High Quality’."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:142
msgid "cheby For soxr only, selects passband rolloff none (Chebyshev) & higher-precision approximation for ’irrational’ ratios. Default value is 0."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:145
msgid "async For swr only, simple 1 parameter audio sync to timestamps using stretching, squeezing, filling and trimming. Setting this to 1 will enable filling and trimming, larger values represent the maximum amount in samples that the data may be stretched or squeezed for each second. Default value is 0, thus no compensation is applied to make the samples match the audio timestamps."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:148
msgid "first_pts For swr only, assume the first pts should be this value. The time unit is 1 / sample rate. This allows for padding/trimming at the start of stream. By default, no assumption is made about the first frame’s expected pts, so no padding or trimming is done. For example, this could be set to 0 to pad the beginning with silence if an audio stream starts after the video stream or to trim any samples with a negative pts due to encoder delay."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:151
msgid "min_comp For swr only, set the minimum difference between timestamps and audio data (in seconds) to trigger stretching/squeezing/filling or trimming of the data to make it match the timestamps. The default is that stretching/squeezing/filling and trimming is disabled (min_comp = FLT_MAX)."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:154
msgid "min_hard_comp For swr only, set the minimum difference between timestamps and audio data (in seconds) to trigger adding/dropping samples to make it match the timestamps. This option effectively is a threshold to select between hard (trim/fill) and soft (squeeze/stretch) compensation. Note that all compensation is by default disabled through min_comp. The default is 0.1."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:157
msgid "comp_duration For swr only, set duration (in seconds) over which data is stretched/squeezed to make it match the timestamps. Must be a non-negative double float value, default value is 1.0."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:160
msgid "max_soft_comp For swr only, set maximum factor by which data is stretched/squeezed to make it match the timestamps. Must be a non-negative double float value, default value is 0."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:163
msgid "matrix_encoding Select matrixed stereo encoding."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:168
msgid "‘none’ select none"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:171
msgid "‘dolby’ select Dolby"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:174
msgid "‘dplii’ select Dolby Pro Logic II"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:179
msgid "filter_type For swr only, select resampling filter type. This only affects resampling operations."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:184
msgid "‘cubic’ select cubic"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:187
msgid "‘blackman_nuttall’ select Blackman Nuttall windowed sinc"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:190
msgid "‘kaiser’ select Kaiser windowed sinc"
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:193
msgid "kaiser_beta For swr only, set Kaiser window beta value. Must be a double float value in the interval [2,16], default value is 9."
msgstr ""

#: ../../source/documentation/components/ffmpeg-resampler.rst:196
msgid "output_sample_bits For swr only, set number of used output sample bits for dithering. Must be an integer in the interval [0,64], default value is 0, which means it’s not used."
msgstr ""

